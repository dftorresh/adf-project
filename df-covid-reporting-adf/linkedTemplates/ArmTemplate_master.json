{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-covid-reporting-adf"
		},
		"ls_ablob_dfadfprojectstorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_dfadfprojectstorage'"
		},
		"ls_adls_dfadfprojectstoragelake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_dfadfprojectstoragelake'"
		},
		"ls_adls_dfadfprojectstoragelake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dfadfprojectstoragelake.dfs.core.windows.net/"
		},
		"ls_http_datalake_case_deaths_csv_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com"
		},
		"tr_copy_population_data_file_pipeline_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/4730e184-32a7-4df6-b494-0d84adea1a60/resourceGroups/adf-course/providers/Microsoft.Storage/storageAccounts/dfadfprojectstorage"
		},
		"tr_copy_covid_data_sa_to_dl_properties_pl_copy_covid_data_parameters_sourceRelativeUrl": {
			"type": "string",
			"defaultValue": "cloudboxacademy/covid19/raw/main/raw/main/ecdc_data/hospital_admissions.csv"
		},
		"tr_copy_covid_data_sa_to_dl_properties_pl_copy_covid_data_parameters_sinkFileName": {
			"type": "string",
			"defaultValue": "hospital_admissions.csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-covid-reporting-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_dfadfprojectstorage_connectionString": {
						"value": "[parameters('ls_ablob_dfadfprojectstorage_connectionString')]"
					},
					"ls_adls_dfadfprojectstoragelake_accountKey": {
						"value": "[parameters('ls_adls_dfadfprojectstoragelake_accountKey')]"
					},
					"ls_adls_dfadfprojectstoragelake_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_dfadfprojectstoragelake_properties_typeProperties_url')]"
					},
					"ls_http_datalake_case_deaths_csv_properties_typeProperties_url": {
						"value": "[parameters('ls_http_datalake_case_deaths_csv_properties_typeProperties_url')]"
					},
					"tr_copy_population_data_file_pipeline_properties_typeProperties_scope": {
						"value": "[parameters('tr_copy_population_data_file_pipeline_properties_typeProperties_scope')]"
					},
					"tr_copy_covid_data_sa_to_dl_properties_pl_copy_covid_data_parameters_sourceRelativeUrl": {
						"value": "[parameters('tr_copy_covid_data_sa_to_dl_properties_pl_copy_covid_data_parameters_sourceRelativeUrl')]"
					},
					"tr_copy_covid_data_sa_to_dl_properties_pl_copy_covid_data_parameters_sinkFileName": {
						"value": "[parameters('tr_copy_covid_data_sa_to_dl_properties_pl_copy_covid_data_parameters_sinkFileName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}