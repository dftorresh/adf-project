{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-covid-reporting-adf"
		},
		"ls_ablob_dfadfprojectstorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_dfadfprojectstorage'"
		},
		"ls_adls_dfadfprojectstoragelake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_dfadfprojectstoragelake'"
		},
		"ls_covid_sql_db_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_covid_sql_db'"
		},
		"ls_databricks_population_ws_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'ls_databricks_population_ws'"
		},
		"ls_adls_dfadfprojectstoragelake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dfadfprojectstoragelake.dfs.core.windows.net/"
		},
		"ls_databricks_population_ws_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1124-011003-g5rxysp7"
		},
		"ls_http_datalake_case_deaths_csv_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com"
		},
		"tr_population_data_arrived_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/4730e184-32a7-4df6-b494-0d84adea1a60/resourceGroups/adf-course/providers/Microsoft.Storage/storageAccounts/dfadfprojectstorage"
		},
		"WorldCupTeams_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://worldcupjson.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-covid-reporting-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_dfadfprojectstorage_connectionString": {
						"value": "[parameters('ls_ablob_dfadfprojectstorage_connectionString')]"
					},
					"ls_adls_dfadfprojectstoragelake_accountKey": {
						"value": "[parameters('ls_adls_dfadfprojectstoragelake_accountKey')]"
					},
					"ls_covid_sql_db_connectionString": {
						"value": "[parameters('ls_covid_sql_db_connectionString')]"
					},
					"ls_databricks_population_ws_accessToken": {
						"value": "[parameters('ls_databricks_population_ws_accessToken')]"
					},
					"ls_adls_dfadfprojectstoragelake_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_dfadfprojectstoragelake_properties_typeProperties_url')]"
					},
					"ls_databricks_population_ws_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_databricks_population_ws_properties_typeProperties_existingClusterId')]"
					},
					"ls_http_datalake_case_deaths_csv_properties_typeProperties_url": {
						"value": "[parameters('ls_http_datalake_case_deaths_csv_properties_typeProperties_url')]"
					},
					"WorldCupTeams_properties_typeProperties_url": {
						"value": "[parameters('WorldCupTeams_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "df-covid-reporting-adf_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"tr_population_data_arrived_properties_typeProperties_scope": {
						"value": "[parameters('tr_population_data_arrived_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'df-covid-reporting-adf_ArmTemplate_0')]"
			]
		}
	]
}