{
	"name": "tr_copy_file_pipeline",
	"properties": {
		"description": "Trigger the pipeline in charge of copy the population data file from Azure Blob Storage whenever a file is added.",
		"annotations": [],
		"runtimeState": "Started",
		"pipelines": [
			{
				"pipelineReference": {
					"referenceName": "pl_ingest_data",
					"type": "PipelineReference"
				}
			}
		],
		"type": "BlobEventsTrigger",
		"typeProperties": {
			"blobPathBeginsWith": "/population/blobs/",
			"blobPathEndsWith": "population_by_age.tsv.gz",
			"ignoreEmptyBlobs": true,
			"scope": "/subscriptions/4730e184-32a7-4df6-b494-0d84adea1a60/resourceGroups/adf-course/providers/Microsoft.Storage/storageAccounts/dfadfprojectstorage",
			"events": [
				"Microsoft.Storage.BlobCreated"
			]
		}
	}
}